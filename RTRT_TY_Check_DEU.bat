@echo off
color f2

@echo on
@echo Which browser do you want to open the CER RTRT Thank You Pages?
@echo 1: IE
@echo 2: Chrome
@echo 3: FireFox
@echo 4: Safari
@echo 5: Opera
@echo off

:START
set /p option=

if "%option%" == "1" goto IE else(
if "%option%" == "2" goto Chrome else(
if "%option%" == "3" goto FireFox else(
if "%option%" == "4" goto Safari else(
if "%option%" == "5" goto Opera else(

@echo on
@echo Are you stupid or something???
@echo Please input the number 1 to 5 to select the browser in above browser list.
@echo off

goto START

)))))


:IE
set browser=iexplore.exe
@echo browser has been set to IE
goto END

:Chrome
set browser=chrome.exe
@echo browser has been set to Chrome
goto END

:FireFox
set browser=firefox.exe
@echo browser has been set to FireFox
goto END

:Safari
set browser=safari.exe
@echo browser has been set to Safari
goto END

:Opera
set browser=opera.exe
@echo browser has been set to Opera
goto END

